<!-- ##### Header Area Start ##### -->
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- The above 4 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <!-- Title -->
    <title>Script House | <%= title %></title>

    <!-- Favicon -->
    <link rel="icon" href="http://127.0.0.1:2000/img/core-img/favicon.ico">

    <!-- Core Stylesheet -->
    <link rel="stylesheet" href="http://127.0.0.1:2000/style.css">
    <link rel="stylesheet" href="http://127.0.0.1:2000/substyle.css">
    <!-- Scoket.io -->
    <script src="http://127.0.0.1:2000/js/socket.io.js"></script>
    <!-- the app namespace -->
    <script src="http://127.0.0.1:2000/js/namespace.js"></script>
    <!-- Markup.js -->
    <script src="http://127.0.0.1:2000/js/markup.js"></script>

</head>

<body style="overflow-x: hidden;">        

<!-- Preloader -->
    <div class="preloader d-flex align-items-center justify-content-center">
        <div class="preloader-circle"></div>
        <div class="preloader-img">
            <img src="http://127.0.0.1:2000/img/core-img/script.png" alt="">
        </div>
    </div>

    <header class="header-area">

        <!-- ***** Navbar Area ***** -->
        <div class="alazea-main-menu">
            <div class="classy-nav-container breakpoint-off">
                <div class="container">
                    <!-- Menu -->
                    <nav class="classy-navbar justify-content-between" id="alazeaNav">

                        <!-- Nav Brand -->
                        <a href="http://127.0.0.1:2000/" class="nav-brand" style="color:white;">Script House</a>

                        <!-- Navbar Toggler -->
                        <div class="classy-navbar-toggler">
                            <span class="navbarToggler"><span></span><span></span><span></span></span>
                        </div>

                        <!-- Menu -->
                        <div class="classy-menu">

                            <!-- Close Button -->
                            <div class="classycloseIcon">
                                <div class="cross-wrap"><span class="top"></span><span class="bottom"></span></div>
                            </div>

                            <!-- Navbar Start -->
                            <div class="classynav">
                                <ul>
                                    <li><a href="http://127.0.0.1:2000/">Home</a></li>
                                    <li><a href="http://127.0.0.1:2000/about">About</a></li>

                                    <li><a href="http://127.0.0.1:2000/nubia">Nubia Design</a></li>
                                    <li><a href="http://127.0.0.1:2000/portfolio">Portfolio</a></li>
                                    <li><a href="http://127.0.0.1:2000/contact">Contact</a></li>
                                </ul>

                                <!-- Search Icon -->
                                <div id="searchIcon">
                                    <i class="fa fa-search" aria-hidden="true"></i>
                                </div>

                            </div>
                            <!-- Navbar End -->
                        </div>
                    </nav>

                    <!-- Search Form -->
                    <div class="search-form">
                        <form action="#" method="get">
                            <input type="search" name="search" id="search" placeholder="Type keywords &amp; press enter...">
                            <button type="submit" class="d-none"></button>
                        </form>
                        <!-- Close Icon -->
                        <div class="closeIcon"><i class="fa fa-times" aria-hidden="true"></i></div>
                    </div>
                </div>
            </div>
        </div>
    </header>